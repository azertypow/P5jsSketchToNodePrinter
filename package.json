{
  "name": "NodePrinter",
  "version": "1.0.0",
  "dependencies": {
    "@types/p5": "^0.9.0",
    "@types/socket.io-client": "^1.4.32",
    "express": "^4.17.1",
    "pdf-to-printer": "^1.1.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/socket.io": "^2.1.4",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4"
  },
  "scripts": {
    "dev": "npm run dev--nodemon & npm run dev--watch",
    "dev--watch": "parcel ./src/main.js --target node",
    "dev--nodemon": "nodemon ./dist/main.js",
    "build": " parcel build ./src/main.js --target node",
    "run-server": "node ./dist/main.js"
  }
}
